<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">

    <!--设置线条样式-->
    <stroke
        android:width="0.5dp"
        android:color="@color/text_orange"/>

    <!--设置弧度-->
    <corners android:radius="200dp"/>

    <!--设置背景色-->
    <solid android:color="@android:color/white"/>

    <padding
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp"/>

</shape>